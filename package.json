{
  "name": "@viktor-titov/jui-lib",
  "version": "0.1.0",
  "description": "Experiment with config for jaeger-ui components lib",
  "main": "dist/main.common.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "unpkg": "dist/main.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/viktor-titov/jui-lib"
  },
  "files": [
    "dist",
    "lib"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/viktor-titov"
  },
  "scripts": {
    "start": "webpack-dev-server --open --mode development --hot",
    "build": "npm run build:types && npm run build:lib && npm run bundle",
    "build:website": "webpack --mode production",
    "build:lib": "npm run build:types && npm run compile",
    "build:types": "tsc --emitDeclarationOnly",
    "compile": "babel src --out-dir lib --extensions \".js,.jsx,.ts,.tsx\" --source-maps inline",
    "bundle": "webpack --config webpack.lib.config.js",
    "eslint": "eslint . --ext .js,.cjs,.ts,.jsx,.tsx, --quiet --fix --ignore-path ./.gitignore",
    "prettier": "prettier --config .prettierrc --write 'src/**/*.{js,jsx,ts,tsx,css,html,json}' ",
    "stylelint": "stylelint src/**/*.{css,less,scss,sass} --fix",
    "prepare": "git init && husky install",
    "type-check": "tsc"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.11",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.11",
    "@babel/runtime": "^7.22.11",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@types/lodash": "^4.14.187",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/react-icons": "^2.2.7",
    "@types/sinon": "^10.0.13",
    "@types/tinycolor2": "^1.4.6",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "babel-loader": "^9.1.3",
    "babel-plugin-add-module-exports": "^1.0.4",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "lint-staged": "^13.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^3.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sinon": "^14.0.1",
    "style-loader": "^3.3.3",
    "stylelint": "^14.16.1",
    "stylelint-config-recommended": "^9.0.0",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@grafana/data": "^9.4.2",
    "@grafana/e2e-selectors": "^9.4.1",
    "@grafana/runtime": "^9.4.1",
    "@grafana/ui": "^9.4.1",
    "chance": "^1.1.11",
    "classnames": "^2.5.1",
    "combokeys": "^3.0.0",
    "copy-to-clipboard": "^3.3.3",
    "deep-freeze": "^0.0.1",
    "fuzzy": "^0.1.3",
    "hoist-non-react-statics": "^3.3.2",
    "json-markup": "^1.1.4",
    "lodash": "^4.14.2",
    "lru-memoize": "^1.1.0",
    "memoize-one": "^6.0.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "prop-types": "^15.8.1",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-icons": "^2.2.7",
    "reselect": "^5.1.0",
    "tinycolor2": "^1.6.0",
    "tslib": "^2.6.2",
    "tween-functions": "^1.2.0"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "lint-staged": {
    "./**/*.{js,cjs,ts,jsx,tsx}": [
      "eslint --fix --ignore-path ./.gitignore"
    ],
    "./**/*.{css,less,scss,sass}": [
      "stylelint --fix"
    ],
    "./**/*.{js,cjs,ts,css,scss,sass,html,json,md}": [
      "prettier . --loglevel warn --write "
    ]
  }
}
